//takes three parameters
//  object
//  direction
//  speed
object = argument0;
direction = argument1;
speed = argument2;

if (direction = RIGHT)
{
    object.x += speed;
}
if (direction = UP)
{
    object.y -= speed;
}
if (direction = LEFT)
{
    object.x -= speed;
}
if (direction = DOWN)
{
    object.y += speed;
}
